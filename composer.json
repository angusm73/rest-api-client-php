{
    "name": "uwd/smsglobal-rest-api-v2",
    "type": "library",
    "description": "Wrapper for SMSGlobal's REST API v2",
    "keywords": ["smsglobal", "sms", "api"],
    "homepage": "http://www.smsglobal.com/",
    "license": "MIT",
    "authors": [
        {
            "name": "SMSGlobal",
            "homepage": "http://www.smsglobal.com/"
        },
        {
            "name": "Ulladulla Web Design",
            "homepage": "https://ulladullawebdesign.com/"
        }
    ],
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "phpdocumentor/phpdocumentor": "*"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4" : {
            "Smsglobal\\RestApiClient2\\" : "RestApiClient2/"
        }
    },
    "replace": {
